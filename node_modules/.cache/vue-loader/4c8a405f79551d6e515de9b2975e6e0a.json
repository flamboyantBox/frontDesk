","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 归档列表 -->\r\n    <v-card class=\"blog-container\">\r\n      <timeline>\r\n        <timeline-title style=\"color: #03a9f4\"> 目前共计{{ count }}篇文章，继续加油 </timeline-title>\r\n        <timeline-item v-for=\"item of archiveList\" :key=\"item.id\">\r\n          <!-- 日期 -->\r\n          <span class=\"time\">{{ item.createTime | date }}</span>\r\n          <!-- 文章标题 -->\r\n          <router-link\r\n            :to=\"'/articles/' + item.id\"\r\n          >\r\n            {{ item.articleTitle }}\r\n          </router-link>\r\n        </timeline-item>\r\n      </timeline>\r\n      <!-- 分页按钮 -->\r\n      <v-pagination\r\n        color=\"#00C4B6\"\r\n        v-model=\"current\"\r\n        :length=\"Math.ceil(count / 10)\"\r\n        total-visible=\"7\"\r\n      />\r\n    </v-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Timeline, TimelineItem, TimelineTitle } from \"vue-cute-timeline\";\r\nimport {getArchives} from \"../../api/article\";\r\nexport default {\r\n  created() {\r\n    this.listArchives();\r\n  },\r\n\r\n  components: {\r\n    Timeline,\r\n    TimelineItem,\r\n    TimelineTitle\r\n  },\r\n\r\n  data: function() {\r\n    return {\r\n      current: 1,\r\n      count: 0,\r\n      archiveList: []\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    listArchives() {\r\n      getArchives().then(res => {\r\n        this.archiveList = res.data.list;\r\n        this.count = res.data.list.length;\r\n      });\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    current(value) {\r\n      getArchives({current: value}).then(res => {\r\n        this.archiveList = res.data.list;\r\n        this.count = res.data.list.length;\r\n      })\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n  .time {\r\n  font-size: 0.75rem;\r\n  color: steelblue;\r\n  margin-right: 1rem;\r\n}\r\n</style>\r\n"]}]}